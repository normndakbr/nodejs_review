<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>My Apps</title>
  </head>
  <body>
    <h1>Master Data Sections</h1>
    <table style="width: 100%" border="1">
      <tr>
        <th>No</th>
        <th>Code</th>
        <th>Name</th>
        <th>Type</th>
        <th>Created At</th>
        <th>Updated At</th>
      </tr>
      <% instanceSections.forEach( section => { %>
      <tr>
        <td style="text-align: center"><%= section.id %></td>
        <td style="text-align: center"><%= section.code %></td>
        <td><%= section.name %></td>
        <td style="text-align: center"><%= section.type %></td>
        <td style="text-align: center"><%= section.createdAt %></td>
        <td style="text-align: center"><%= section.updatedAt %></td>
      </tr>
      <% }); %>
    </table>
  </body>
</html>
